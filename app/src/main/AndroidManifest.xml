<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.a6_dashboard">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.6_DashBoard">
        <activity
            android:name=".activities.MusicActivity"
            android:exported="true" />
        <activity
            android:name=".activities.Settings_Activity"
            android:exported="true"
            android:theme="@style/Theme.Settings" />
        <activity
            android:name=".activities.Restaurants_Activity"
            android:exported="true"
            android:theme="@style/Theme.Restaurants" />
        <activity
            android:name=".activities.Maps_Activity"
            android:exported="true"
            android:theme="@style/Theme.Maps" />
        <activity
            android:name=".activities.Gallery_Activity"
            android:exported="true"
            android:theme="@style/Theme.Gallery" />
        <activity
            android:name=".activities.Quizzes_Activity"
            android:exported="true"
            android:theme="@style/Theme.Quizzes" />
        <activity
            android:name=".activities.ResultadoActivity"
            android:exported="true"
            android:theme="@style/Theme.Quizzes" />
        <activity
            android:name=".activities.EdadCanina_Activity"
            android:exported="true"
            android:theme="@style/Theme.EdadCanina" />
        <activity
            android:name=".activities.Amigos_Activity"
            android:exported="true"
            android:theme="@style/Theme.Amigos" />
        <activity
            android:name=".activities.Calculadora_Activity"
            android:exported="true"
            android:theme="@style/Theme.Calculadora" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">

            <!-- <intent-filter> -->
            <!-- <action android:name="android.intent.action.MAIN" /> -->


            <!-- <category android:name="android.intent.category.LAUNCHER" /> -->
            <!-- </intent-filter> -->
        </activity>
        <activity
            android:name=".SplashActivity"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>